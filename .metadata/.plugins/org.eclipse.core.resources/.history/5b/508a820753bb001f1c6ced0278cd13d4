package techLink;

public class GuessTheNumber {
	
	public static void main(String[] args) {
		
		GuessTheNumber gtn = new GuessTheNumber();
		gtn.number(4123);
	}
	
	public void number(int num) {
		String numStr=Integer.toString(num);
		for(int i=0;i<4;i++) {
			System.out.println(numStr.charAt(i));
		}
	}

}
